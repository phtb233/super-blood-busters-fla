		<questions>
			<!-- Holds the data for the questions shown when a power up is hit.-->
			<!-- First 10 taken from http://www.educationquizzes.com/gcse/biology/unit-3-blood/ -->
			<!-- Question 1 -->
			<question correctAnswer="2" time="7">
				<text>
					Blood plasma is...
				</text>
				<answers one="solid" two="liquid" three="gas"/>
			</question>
			<!-- Question 2 -->
			<question correctAnswer="2" time="5">
				<text>
					Plasma carries carbon dioxide away from the...
				</text>
				<answers one="lungs" two="organs" three="platelets"/>
			</question>
			<!-- Question 3 -->
			<question correctAnswer="3" time="7">
				<text>
					Arteries carry blood...
				</text>
				<answers one="towards the heart" two="from atria to ventricles" three="away from the heart"/>
			</question>
			<!-- Question 4 -->
			<question correctAnswer="3" time="7">
				<text>
					Valves prevent the backflow of blood, and are found in...
				</text>
				<answers one="arteries" two="capillaries" three="veins"/>
			</question>
			<!-- Question 5 -->
			<question correctAnswer="3" time="7">
				<text>
					Which blood vessels are so small that a microscope is needed to see them?
				</text>
				<answers one="arteries" two="veins" three="lymphatic vessels"/>
			</question>
			<!-- Question 6 -->
			<question correctAnswer="3" time="6">
				<text>
					Their walls are very thin to allow diffusion of gases.
				</text>
				<answers one="arteries" two="veins" three="capillaries"/>
			</question>
			<!-- Question 7 -->
			<question correctAnswer="3" time="6">
				<text>
					Arteries carry blood which is...
				</text>
				<answers one="deoxygenated" two="red" three="oxygenated"/>
			</question>
			<!-- Question 8 -->
			<question correctAnswer="1" time="6">
				<text>
					Another name for a red blood cell is...
				</text>
				<answers one="erythrocyte" two="leucocyte" three="platelet"/>
			</question>
			<!-- Question 9 -->
			<question correctAnswer="3" time="6">
				<text>
					Platelets are involved in blood...
				</text>
				<answers one="flow" two="renewal" three="clotting"/>
			</question>
			<!-- Question 10 -->
			<question correctAnswer="1" time="6">
				<text>
					Red cells have a special shape to allow maximum...
				</text>
				<answers one="surface area for diffusion" two="volume for water" three="density for floating"/>
			</question>
			<!-- Question 11 -->
			<question correctAnswer="2" time="8">
				<text>
					Which heart chamber has the thickest muscle?
				</text>
				<answers one="left atrium" two="left ventricle" three="right atrium"/>
			</question>
			<!-- Question 12 -->
			<question correctAnswer="3" time="9">
				<text>
					The correct order for the movement of blood through the heart is...
				</text>
				<answers one="Left Atrium, Left Ventricle Right Ventricle, Right Atrium"
				two="Right Atrium, Left Ventricle, Left Atrium, Right Ventricle"
				three="Left Ventricle, Right Atrium, Right Ventricle, Left Atrium"/>
			</question>
			<!-- Question 13 -->
			<question correctAnswer="1" time="8">
				<text>
					Which of the following substances leaves blood as it passes through capillaries in organs like the liver or pancreas?
				</text>
				<answers one="oxygen" two="carbon dioxide" three="starch"/>
			</question>
			<!-- Question 14 -->
			<question correctAnswer="3" time="7">
				<text>
					Anaerobic respiration in humans produces....
				</text>
				<answers one="ethanol" two="lactic acid, carbon dioxide" three="lactic acid"/>
			</question>
			<!-- Question 15 -->
			<question correctAnswer="1" time="8">
				<text>
					In distance running, the best description of the type of respiration occuring is...
				</text>
				<answers one="mostly aerobic" two="mostly anaerobic" three="equally aerobic and anaerobic"/>
			</question>
				<!-- Question 16 -->
			<question correctAnswer="3" time="20">
				<text>
					If the volume of the left ventricle is 50cm3 and the resting heart rate is 70 beats per minute what volume of blood will leave the left ventricle of a resting man in 10 minutes?
				</text>
				<answers one="350cm3" two="3500cm3" three="35000cm3"/>
			</question>
			<!-- Question 17 -->
			<question correctAnswer="1" time="10">
				<text>
					Which of the following is the correct order of blood vessels arranged from lowest pressure to highest pressure?
				</text>
				<answers one="vein, venule, capillary, artery" 
				two="venule, vein, artery, capillary"
				three="vein, arteriole, venule, capillary" 
				/>
			</question>
			<!-- Question 18  -->
			<question correctAnswer="3" time="9">
				<text>
					Which of the following is an effect of regular exercise?
				</text>
				<answers one="Recovery takes longer"
				two="Resting heart rate gets faster" 
				three="Resting heart muscles contract harder"/>
			</question>
			<!-- Question 19 -->
			<question correctAnswer="2" time="9">
				<text>
					More blood is delivered to leg muscles during exercise because...
				</text>
				<answers one="more water is needed for sweating" 
				two="more oxygen is needed by muscle cells" 
				three="warmer muscles work faster" 
				/>
			</question>
			<!-- Question 20 -->
			<question correctAnswer="3" time="8">
				<text>
					The organ mainly responsible for removing lactic acid following vigorous exercise is...
				</text>
				<answers one="Lungs" two="Heart" three="Liver"/>
			</question>
			<!-- Question 21 -->
			<question correctAnswer="3" time="9">
				<text>
					Humans have a double circulatory system. The advantage of this is...
				</text>
				<answers one="it oxygenates blood"
				two="it removes carbon dioxide easily" 
				three="it keeps blood pressure high in body capillaries"/>
			</question>
			
			
			
		
